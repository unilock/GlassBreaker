{
  "schema_version": 1,
  "mixin": [
    "glassbreaker.mixins.json",
    "glassbreaker-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "glassbreaker",
    "version": "${version}",
    "name": "Example Mod",
    "description": "This is an example description! Tell everyone what your mod is about!",
    "authors": ["Me!"],
    "contact": {
      "sources": "https://github.com/architectury/architectury-templates"
    },
    "license": "Insert License Here",
    "icon": "assets/glassbreaker/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.unilock.glassbreaker.quilt.GlassBreakerQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.18.2"
      }
    ]
  }
}
